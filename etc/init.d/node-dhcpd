#!/sbin/openrc-run

. /lib/rc/sh/rc-linkname-parsers.sh
parse_svcdata
node="${svcdata[node]}"
thing="${svcdata[name]}"

depend() {
if [[ "$RC_SVCNAME" != node-dhcpd ]];then
        need "${thing}"
        provide "node-dhcpd_${node}"
fi
}

start(){
	einfo "Starting dhcpd service for node \"${node}\"."
}
